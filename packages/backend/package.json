    "@sinclair/typebox": "^0.32.30",
    "@types/web-push": "^3.6.4",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.0",
    "googleapis": "^170.0.0",
    "jose": "^6.1.3",
    "nodemailer": "^6.9.15",
    "pdfkit": "^0.17.2",
    "web-push": "^3.6.7"
